    "tokenColors": [
        {
            "settings": {
                "foreground": "#F5F5F4"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown"
            ],
            "settings": {
                "foreground": "#F5F5F4"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#78716C"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#FDE047"
            }
        },
        {
            "name": "Template Definition",
            "scope": [
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#DB2777"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": ["meta.template.expression"],
            "settings": {
                "foreground": "#F5F5F4"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#A78BFA"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#A78BFA"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "foreground": "#A78BFA"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#F5F5F4"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#DB2777"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#DB2777"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#67E8F9"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#A3E635"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#A3E635"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#A3E635"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#F59E0B"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#DB2777"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#A3E635"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#67E8F9"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#67E8F9"
            }
        },
        {
            "name": "Library class/type",
            "scope": "support.type, support.class",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#67E8F9"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#F44747"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#F44747"
            }
        },
        // TODO: consider making json more consistent with other languages (need to change key colour then though... ie. maybe match yaml: DB2777)
        {
            "name": "JSON String",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#D6D3D1"
            }
        },
        {
            "name": "diff.header",
            "scope": "meta.diff, meta.diff.header",
            "settings": {
                "foreground": "#78716C"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#DB2777"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#A3E635"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#FDE047"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#A78BFAA0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#FDE047"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#DB2777"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#FDE047"
            }
        },
        {
            "name": "Markup Styling",
            "scope": "markup.bold, markup.italic",
            "settings": {
                "foreground": "#67E8F9"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#F59E0B"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#A3E635"
            }
        },
        {
            "name": "Markup Setext Header",
            "scope": "markup.heading.setext",
            "settings": {
                "foreground": "#A3E635",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Quote",
            "scope": "markup.quote.markdown",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#78716C"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Link Title/Description",
            "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
            "settings": {
                "foreground": "#A78BFA"
            }
        },
        {
            "name": "Markdown Underline Link/Image",
            "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
            "settings": {
                "foreground": "#FDE047"
            }
        },
        {
            "name": "Markdown Emphasis",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "name": "Markdown Punctuation Definition Link",
            "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
            "settings": {
                "foreground": "#F5F5F4"
            }
        },
        {
            "name": "Markdown List Punctuation",
            "scope": ["punctuation.definition.list.begin.markdown"],
            "settings": {
                "foreground": "#A3E635"
            }
        },
        // TODO: not sure where these (token.*-token) are used...
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796E6"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#CD9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#F44747"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#B267E6"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#F59E0B"
            }
        }
    ],
    "semanticHighlighting": true
}
