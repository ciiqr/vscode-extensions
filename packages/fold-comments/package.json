{
    "name": "fold-comments",
    "displayName": "Fold Comments",
    "version": "1.0.0",
    "private": true,
    "description": "fold comments",
    "categories": [
        "Other"
    ],
    "keywords": [
        "fold",
        "comment"
    ],
    "bugs": {
        "url": "https://github.com/ciiqr/vscode-extensions/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ciiqr/vscode-extensions"
    },
    "license": "MIT",
    "publisher": "ciiqr",
    "main": "./dist/src/extension.js",
    "scripts": {
        "dev": "tsc --watch",
        "vscode:prepublish": "tsc"
    },
    "contributes": {
        "commands": [
            {
                "title": "Fold Comments: Toggle",
                "command": "fold-comments.toggle"
            },
            {
                "title": "Fold Comments: Fold",
                "command": "fold-comments.fold"
            },
            {
                "title": "Fold Comments: Unfold",
                "command": "fold-comments.unfold"
            }
        ]
    },
    "activationEvents": [],
    "devDependencies": {
        "@pentible/eslint-config-node": "0.102.0",
        "@pentible/eslint-config-prettier": "0.102.0",
        "@types/node": "18.19.17",
        "@types/vscode": "^1.86.0",
        "@typescript-eslint/eslint-plugin": "7.16.1",
        "@typescript-eslint/parser": "7.16.1",
        "eslint": "^8.56.0",
        "typescript": "5.5.3"
    },
    "engines": {
        "vscode": "^1.86.0"
    }
}
