{
    "name": "note-syntax",
    "displayName": "Note",
    "version": "1.6.0",
    "private": true,
    "description": "note syntax",
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "note",
        "notes",
        "todo"
    ],
    "bugs": {
        "url": "https://github.com/ciiqr/vscode-extensions/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ciiqr/vscode-extensions"
    },
    "license": "MIT",
    "publisher": "ciiqr",
    "contributes": {
        "configurationDefaults": {
            "[note]": {
                "editor.bracketPairColorization.enabled": false
            }
        },
        "grammars": [
            {
                "language": "note",
                "scopeName": "source.note",
                "path": "./syntaxes/Note.tmLanguage.json"
            }
        ],
        "languages": [
            {
                "id": "note",
                "aliases": [
                    "Note",
                    "note"
                ],
                "extensions": [
                    ".txt",
                    ".todo"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "dark": "./images/language-icon.png",
                    "light": "./images/language-icon.png"
                }
            }
        ]
    },
    "engines": {
        "vscode": "^1.86.0"
    },
    "icon": "images/icon.png"
}
