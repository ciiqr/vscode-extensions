{
    "name": "vscode-extensions",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "workspaces": [
        "./packages/*"
    ],
    "scripts": {
        "bump-version": "./scripts/bump-version.sh",
        "dev": "turbo dev",
        "lint": "check-package-lock && shellcheck-all && tsc --noEmit && eslint . && prettier --loglevel warn --check .",
        "lint:fix": "eslint --fix . && prettier --loglevel warn --write .",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@pentible/check-package-lock": "0.68.0",
        "@pentible/eslint-config": "0.68.0",
        "@pentible/eslint-config-node": "0.68.0",
        "@pentible/eslint-config-prettier": "0.68.0",
        "@pentible/prettier": "0.68.0",
        "@pentible/shellcheck-all": "0.68.0",
        "@pentible/tsconfig": "0.68.0",
        "generator-code": "1.7.5",
        "husky": "8.0.3",
        "lint-staged": "13.2.1",
        "prettier": "2.8.8",
        "turbo": "1.9.3",
        "typescript": "5.0.4",
        "vsce": "2.11.0",
        "yo": "4.3.1"
    }
}
